class Solution {
    public int[] findThePrefixCommonArray(int[] A, int[] B) {
        int c[]=new int[A.length];
        for(int i=0;i<A.length;i++){
            int count =0;
            Set<Integer> set=new HashSet<>();
            for(int j=0;j<=i;j++){
                set.add(A[j]);
            }
            for(int j=0;j<=i;j++){
                if(!set.add(B[j])){
                    count++;
                }
            }
            c[i]=count;
        }
        return c;
    }
}
